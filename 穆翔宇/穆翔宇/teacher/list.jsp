<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<c:set var="ctx" value="${pageContext.request.contextPath}" />
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en" class="app">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title>教师列表</title>
<link rel="stylesheet" href="${ctx }/css/app.v2.css" type="text/css" />
<link rel="stylesheet" href="${ctx }/js/calendar/bootstrap_calendar.css" type="text/css" cache="false" />
<link rel="stylesheet" href="${ctx }/css/teacher_list.css" type="text/css" />

<!-- 教师列表js -->
<script type="text/javascript">
	function sousuo(){
		var p=$("#searchParam").val();
		window.location.href="${ctx }/Teachermanage/list?searchParam="+p;
		$("[name='pagen']").each(function(key,value){
			$(this).attr("href",$(this).attr("href")+"&searchParam='"+p+"'");
		});
	}
	function pageSkip(){
		var action="${action2}";
		alert(action)
		if(action == "list"){
			var listpage = document.getElementsByName("listpage");
			var listpageNum = document.getElementsByName("listpageNum");
			var pageNum = listpageNum[0].value;
			alert(pageNum);
			listpage[0].href = "${ctx }/Teachermanage/list?pageNum="+pageNum+"&searchParam=${searchParam}";
		}else if(action == "edit2"){
			alert("修改");
			var editpage = document.getElementsByName("editpage");
			var editpageNum = document.getElementsByName("editpageNum");
			var pageNum = editpageNum[0].value;
			alert(pageNum);
			editpage[0].href = "${ctx }/Teachermanage/list?pageNum="+pageNum+"&searchParam=${searchParam}";
		}else if(action == "del2"){
			alert("删除");
			var delpage = document.getElementsByName("delpage");
			var delpageNum = document.getElementsByName("delpageNum");
			var pageNum = delpageNum[0].value;
			alert(pageNum);
			delpage[0].href = "${ctx }/Teachermanage/list?pageNum="+pageNum+"&searchParam=${searchParam}";
		}
	}
</script>

</head>
<body>
<section class="vbox">
	<jsp:include page="../header.jsp"/>	
  	<section>
    <section class="hbox stretch"> <!-- .aside -->
    	<jsp:include page="../aside.jsp"/>
      	<!-- /.aside -->
      	<section id="content">
        	<section class="vbox">
          		<section class="scrollable padder">
          			 <div class="location">
          			 	当前位置：用户管理 -&gt; 用户列表
          			 </div>
          	<form action="${ctx }/Teachermanage/list" method="post">
            		<div class="content">
            			<div class="blank10"></div>
            			<div class="search_block">
            				<div class="h">
            					<span class="icon-sprite icon-magnifier"></span>
            					<h3>快速搜索</h3>
        					</div>            			
            			 	<div class="sousuodiv">
            			 		<div class="sousuobutton">
            			 			<div class="yonghuming">
            			 				姓名：<input type="text" id="searchParam" name="searchParam" value="${searchParam }"/>
            			 			</div>
            			 			<a id="sousuosubmit" onclick="sousuo()">搜索</a>
            			 		</div>
            			 	</div>
            			</div>
        			</div>
        			
        			<div class="blank10"></div>
        			
        			<div class="search_block">
        				<div class="h">
           					<span class="icon-sprite icon-list"></span>
            				<h3>用户列表</h3>
            				<div class="addAndDelete">
                				<a href="#">新增</a>
                				<a href="#">删除</a>
            				</div>
        				</div>
        				
        				<div class="cnt-wp">
        					<div class="cnt">
        						 <table class="data-table" width="100%" border="0" cellspacing="0" cellpadding="0">
        						 	 <tbody>
        						 	 	 <tr>
                            				<td width="11%" class="biaoti">教师工号</td>
                  							<td width="11%" class="biaoti">姓名</td>
                  							<td width="15%" class="biaoti">邮箱</td>
                  							<td width="14%" class="biaoti">手机</td>
                  							<td width="13%" class="biaoti">雇用时间</td>
                  							<td width="15%" class="biaoti">家庭住址</td>
        									<td width="15%" class="biaoti">所教课程</td>
                  							<td width="3%" class="biaoti">修改</td>
                  							<td width="3%" class="biaoti">删除</td>
        						 	 	 </tr>
        						 	 	 <c:forEach items="${page.list}" var="p">
        						 	 	 	<tr>
                   								<td class="chengyuan">${p.id }</td>
                  								<td class="chengyuan">${p.name }</td>
                  								<td class="chengyuan">${p.email}</td>
                  								<td class="chengyuan">${p.phone}</td>
                  								<td class="chengyuan">${p.hiredate }</td>
                  								<td class="chengyuan">${p.address }</td>
                  								<td class="chengyuan">
                  									<c:forEach items="${p.courses }" var="course" varStatus="status">
                  										<c:choose>
                  											<c:when test = "${status.last }">
                  												${course.name }
                  											</c:when>
                  											<c:otherwise>
                  												${course.name }、
                  											</c:otherwise>
                  										</c:choose>
                  									</c:forEach>
                  								</td>
                            					<td class="icon">
                            						<a class="opt" title="编辑" href="${ctx}/Teachermanage/edit?id=${p.id}">
                            							<span class="icon-sprite icon-edit"></span>
                            						</a>
                            					</td>
                            					<td class="icon tail">
                            						<a class="opt" title="删除" href="${ctx}/Teachermanage/del?id=${p.id}">
                            							<span class="icon-sprite icon-delete"></span>
                            						</a>
                            					</td>
                        				 	</tr>
        						 	 	 </c:forEach>
        						 	 </tbody>
        						 </table>
        					</div>
        					<div class="dibu">
        						 <div class="sumpage">共    ${page.totalRecords}  条记录，每页    ${page.pageSize}  条记录</div>
        						 <div class="fenye">
        						 		<c:if test="${action2=='edit2'}">
        						 			<a name=pagen href="${ctx }/Teachermanage/edit2?pageNum=${page.topPageNo}&searchParam=${searchParam}"><span>首页</span></a>
        						 			<a name=pagen href="${ctx }/Teachermanage/edit2?pageNum=${page.previousPageNo }&searchParam=${searchParam}"><span>上一页</span></a>
        						 			<a name=pagen href="${ctx }/Teachermanage/edit2?pageNum=${page.nextPageNo }&searchParam=${searchParam}"><span>下一页</span></a>
        						 			<a name=pagen href="${ctx }/Teachermanage/edit2?pageNum=${page.bottomPageNo }&searchParam=${searchParam}"><span>尾页</span></a>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="editpageNum"/>页
        						 			<a name="editpage" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 		<c:if test="${action2=='del2'}">
        						 			<a name="pagen" href="${ctx }/Teachermanage/del2?pageNum=${page.topPageNo}&searchParam=${searchParam}"><span>首页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/del2?pageNum=${page.previousPageNo}&searchParam=${searchParam}"><span>上一页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/del2?pageNum=${page.nextPageNo }&searchParam=${searchParam}"><span>下一页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/del2?pageNum=${page.bottomPageNo }&searchParam=${searchParam}"><span>尾页</span></a>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="delpageNum"/>页
        						 			<a name="delpage" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 		<c:if test="${action2=='list'}">
        						 			<a name="pagen" href="${ctx }/Teachermanage/list?pageNum=${page.topPageNo}&searchParam=${searchParam}"><span>首页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/list?pageNum=${page.previousPageNo }&searchParam=${searchParam}"><span>上一页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/list?pageNum=${page.nextPageNo }&searchParam=${searchParam}"><span>下一页</span></a>
        						 			<a name="pagen" href="${ctx }/Teachermanage/list?pageNum=${page.bottomPageNo }&searchParam=${searchParam}"><span>尾页</span></a>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="listpageNum"/>页
        						 			<a name="listpage" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 </div>
        					</div>
        				</div>
        			</div>
        		</form>
        		
            	</section>
        		</section>
        	</section>
        </section>
	</section>
</section>

<!-- 左边菜单 -->
<script src="${ctx }/js/app.v2.js"></script> <!-- Bootstrap --> <!-- App --> 
<script src="${ctx }/js/charts/easypiechart/jquery.easy-pie-chart.js" cache="false"></script> 
<script src="${ctx }/js/charts/sparkline/jquery.sparkline.min.js" cache="false"></script>
<script src="${ctx }/js/charts/flot/jquery.flot.min.js" cache="false"></script>
<script src="${ctx }/js/charts/flot/jquery.flot.tooltip.min.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/jquery.flot.resize.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/jquery.flot.grow.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/demo.js" cache="false"></script> 
<script src="${ctx }/js/calendar/bootstrap_calendar.js" cache="false"></script> 
<script src="${ctx }/js/calendar/demo.js" cache="false"></script> 
